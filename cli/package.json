{
  "name": "fles-js-cli",
  "version": "1.3.96",
  "description": "FLES-JS CLI - The Next-Gen Development Platform",
  "type": "module",
  "bin": {
    "fles-js": "dist/index.js"
  },
  "files": [
    "dist",
    "dist/files"
  ],
  "scripts": {
    "build": "bun build ./src/index.js --outdir dist --target node",
    "copy-files": "mkdir -p dist/files/models && cp -r src/providers/mongoets/templates/files/* dist/files/",
    "prepublishOnly": "bun run build"
  },
  "keywords": [
    "cli",
    "mongodb",
    "graphql",
    "generator"
  ],
  "author": "Rafles Yohanes",
  "license": "MIT",
  "dependencies": {
    "fles-mongo-apollo": "^1.0.16",
    "fles-mongo-express": "^1.0.15",
    "chalk": "^5.4.1",
    "commander": "^12.1.0",
    "fs-extra": "^11.2.0",
    "inquirer": "^12.3.0"
  },
  "devDependencies": {
    "glob": "^11.0.0",
    "javascript-obfuscator": "^4.1.1"
  }
}
